{
  "expo": {
    "name": "Condomínio Santa Cecília",
    "slug": "condominio-cecilia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": null,
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.condominio.cecilia",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Este aplicativo usa Face ID / Touch ID para autenticação biométrica.",
        "NSCameraUsageDescription": "Este aplicativo precisa de acesso à sua câmera para tirar fotos para seu perfil.",
        "NSPhotoLibraryUsageDescription": "Este aplicativo precisa de acesso à sua galeria de fotos para selecionar imagens para seu perfil.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E3A8A"
      },
      "package": "br.com.condominio.cecilia",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json",
      "runtimeVersion": "1.0.0",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.app/"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Permitir que o Condomínio Santa Cecília use Face ID para autenticação."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      [
        "expo-splash-screen",
        {
          "autoHideSplash": true,
          "image": null
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "75a10dae-752d-45e2-af70-45a4830e9a5b"
      },
      "router": {
        "origin": "https://rork.app/"
      }
    },
    "owner": "ms-consu",
    "updates": {
      "url": "https://u.expo.dev/75a10dae-752d-45e2-af70-45a4830e9a5b2"
    }
  }
}
