{
  "name": "condominio-santacecilia",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "start-web": "expo start --web --tunnel",
    "start-web-dev": "DEBUG=expo* expo start --web --tunnel",
    "build:android": "npx expo prebuild --clean -p android && cd android && gradlew.bat clean && ./gradlew assembleRelease --stacktrace",
    "build:android:clean": "rimraf android && npx expo prebuild -p android && cd android && gradlew.bat assembleRelease --stacktrace",
    "build:android:eas": "npx eas build --platform android --profile preview --non-interactive",
    "build:android:eas:dev": "npx eas build --platform android --profile development --non-interactive",
    "build:android:eas:debug": "npx eas build --platform android --profile debug --non-interactive",
    "build:android:local": "rimraf android && npx expo prebuild -p android && cd android && set ANDROID_HOME=%LOCALAPPDATA%\\Android\\Sdk && gradlew.bat assembleRelease --stacktrace --info > build-log.txt 2>&1 && type build-log.txt",
    "build:ios": "npx expo prebuild -p ios",
    "build:ios:eas": "npx eas build --platform ios --profile preview --non-interactive",
    "build:ios:eas:dev": "npx eas build --platform ios --profile development --non-interactive",
    "build:ios:eas:debug": "npx eas build --platform ios --profile debug --non-interactive",
    "build:ios:simulator": "npx eas build --platform ios --profile debug --non-interactive",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
